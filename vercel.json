{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/proxy?path=/api/:path*"
    },
    {
      "source": "/",
      "destination": "/login.html"
    },
    {
      "source": "/login",
      "destination": "/login.html"
    },
    {
      "source": "/admin",
      "destination": "/admin.html"
    },
    {
      "source": "/student",
      "destination": "/student.html"
    },
    {
      "source": "/teacher",
      "destination": "/teacher.html"
    }
  ]
}